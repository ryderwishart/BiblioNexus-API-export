  "name": "Matthew 18:12–14",
  "localizedName": "Matthew 18:12–14",
  "content": [
    {
      "tiptap": {
        "type": "doc",
        "content": [
          {
            "type": "paragraph",
            "content": [
              {
                "type": "text",
                "text": "Humans are like sheep that are lost from God’s family."
              }
            ]
          },
          {
            "type": "paragraph",
            "content": [
              {
                "type": "text",
                "text": "God the Father sends his Son into the world to find them."
              }
            ]
          },
          {
            "type": "paragraph",
            "content": [
              {
                "type": "text",
                "text": "He is full of joy about each one who comes home."
              }
            ]
          },
          {
            "type": "paragraph",
            "content": [
              {
                "type": "text",
                "text": "God doesn’t want anyone to ever be apart from his love and life."
              }
            ]
          }
        ]
      }
    }
  ],
